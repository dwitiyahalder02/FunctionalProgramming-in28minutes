package functionalprogramming.structre;

import java.util.ArrayList;
import java.util.Arrays;

class Course
{
	private String name;
	private String category;
	private String reviewScore;
	private int noOfStudents;
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getCategory() {
		return category;
	}
	public void setCategory(String category) {
		this.category = category;
	}
	public String getReviewScore() {
		return reviewScore;
	}
	public void setReviewScore(String reviewScore) {
		this.reviewScore = reviewScore;
	}
	public int getNoOfStudents() {
		return noOfStudents;
	}
	public void setNoOfStudents(int noOfStudents) {
		this.noOfStudents = noOfStudents;
	}
	public Course(String name, String category, String reviewScore, int noOfStudents) {
		super();
		this.name = name;
		this.category = category;
		this.reviewScore = reviewScore;
		this.noOfStudents = noOfStudents;
	}
	@Override
	public String toString() {
		return "Course [name=" + name + ", category=" + category + ", reviewScore=" + reviewScore + ", noOfStudents="
				+ noOfStudents + "]";
	}
	
}
public class FP07customClass {

	public static void main(String args[]) {
		ArrayList<Course> list=new ArrayList<>();
		list.add(new Course("java","Reading","2000",200));
		list.add(new Course("SpringBoot","Writing","3000",300));
		list.add(new Course("SpringDataJpa","story","5000",900));
		list.add(new Course("Microservices","micro","7000",100));
				
		
	}
}
