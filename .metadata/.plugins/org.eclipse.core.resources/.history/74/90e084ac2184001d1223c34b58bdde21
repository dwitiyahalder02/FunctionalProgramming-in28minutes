package functionalprogramming.structre;

import java.util.Arrays;
import java.util.List;

public class FP10HigherOrderfunc {

	public static void main(String args[])
	{
		List<String> courses=Arrays.asList("Spring","Springboot","aws","spring data jp","microservices");
		
		System.out.println(courses.stream().filter(course->course.length()>10).map(String::toUpperCase));
	}
}
